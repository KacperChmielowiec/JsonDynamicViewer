<ResourceDictionary xmlns="https://github.com/avaloniaui"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:sys="clr-namespace:System;assembly=mscorlib">
	
	<!-- Azure -->
	<sys:String x:Key="String/Azure.AzureCliNotFound">找不到 Azure 命令列介面 (CLI)。请确认 Azure 命令列介面已经安装且其路径已加入至文件 '/etc/paths'。</sys:String>

	<!-- SessionView -->
	<sys:String x:Key="String/SessionView.AddLogFiles.WithShortcut">加入日志档案 (⌘O)</sys:String>
	<sys:String x:Key="String/SessionView.EditPathEnvVar">编辑 /etc/paths…</sys:String>
	<sys:String x:Key="String/SessionView.MemoryUsage">总内存使用量</sys:String>
	<sys:String x:Key="String/SessionView.PredefinedLogTextFilters">已定义的文字筛选 (⌘P)</sys:String>
	<sys:String x:Key="String/SessionView.RestartAsAdministrator">以 超级管理员 身份重新启动</sys:String>
	<sys:String x:Key="String/SessionView.ShowAllLogsTemporarily">暂时显示所有日志 (⌥A)</sys:String>
	<sys:String x:Key="String/SessionView.ShowFileInExplorer">在 Finder 中显示档案</sys:String>
	<sys:String x:Key="String/SessionView.ShowMarkedLogsTemporarily">暂时只显示已标记的日志 (⌥M)</sys:String>
	<sys:String x:Key="String/SessionView.ShowWorkingDirectoryInExplorer">在 Finder 中开启工作目录</sys:String>

	<!-- StandardOutputLogDataSource -->
	<sys:String x:Key="String/StandardOutputLogDataSource.CommandNotFound">找不到指令 '{0}'。请确认指令或可执行档已安装且其路径已加入至文件 '/etc/paths'。</sys:String>
	
</ResourceDictionary>
